{
    "name": "extlibraries",
    "icon": "library.svg",
    "displayName": "External libraries",
    "description": "Add external libraries to project as symlinks",
    "version": "0.0.2",
    "publisher": "dannielarriola",
    "engines": {
        "vscode": "^1.12.0"
    },
    "bugs": {
        "url": "https://github.com/dannielarriola/vscode-extlibraries/issues",
        "email": "dannielarriola@gmail.com"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "homepage": "https://github.com/dannielarriola/vscode-extlibraries/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/dannielarriola/vscode-extlibraries.git"
    },
    "activationEvents": [
        "onCommand:extension.addExtLibrary"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.addExtLibrary",
            "title": "Extlibraries: Add external library"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}